using MongoDB.Bson.Serialization.Attributes;

namespace Twillink.Shared.Models
{
    public class Category : BaseModel
    {
        [BsonId]
        public string? Id {get; set;}

        [BsonElement("Name")]
        public string? Name {get; set;}
        
        [BsonElement("Ket")]
        public string? Ket {get; set;}
    
    }
}